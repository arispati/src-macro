{
    "name": "arispati/src-macro",
    "description": "Kumpulan Macro untuk SRC",
    "license": "MIT",
    "keywords": [
        "arispati",
        "src-macro"
    ],
    "authors": [
        {
            "name": "Muhamad Aris",
            "email": "masarispati@gmail.com"
        }
    ],
    "require": {
        "php": ">=7"
    },
    "require-dev": {
        "laravel/laravel": "^5.6",
        "laravel/lumen": "^5.7",
        "squizlabs/php_codesniffer": "3.*"
    },
    "suggest": {
        "spatie/laravel-collection-macros": "A set of useful Laravel collection macros.",
        "maatwebsite/excel": "Supercharged Excel exports and imports in Laravel"
    },
    "autoload": {
        "psr-4": {
            "Arispati\\SrcMacro\\": "src"
        }
    },
    "scripts": {
        "cs": "phpcs --standard=PSR12 src/"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Arispati\\SrcMacro\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
